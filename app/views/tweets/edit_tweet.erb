<h1>Edit Tweet</h1>

<h2>Tweet: <%= @tweet.content %></h2>

<form action="/tweets/<%= @tweet.id %>/edit" method="post">
    <input id="hidden" type="hidden" name="_method" value="patch">
    <br>

    <h3>New Content</h3>
    <input type="text" name="content" id="content"></input>
    <br><br>


    <input type="submit" value="submit">
</form>